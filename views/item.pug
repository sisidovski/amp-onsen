doctype html
html
  head
    link(rel='ampHtml' href=target.ampUrl)
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width,minimum-scale=1,initial-scale=1')
    title= "Signed HTTP Exchanges"
    style.
      h1,p {
        padding: 18px;
      }
      header {
        background-color: #318e8599;
      }
      header > h1 {
        color: white;
      }
      img {
        width: 100%;
      }
  body
    header
      h1= target.name
    img(src=target.img)
    p #{target.name}
